<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://Java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>p1Laith</display-name>
  <welcome-file-list>
    <welcome-file>frontend/login.html</welcome-file>
    <welcome-file>frontend/employeeHomeScreen.html</welcome-file>
    <welcome-file>frontend/financeManager.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmployeeServlet</display-name>
    <servlet-name>EmployeeServlet</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeServlet</servlet-name>
    <url-pattern>/employee/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FinanceManagerServlet</display-name>
    <servlet-name>FinanceManagerServlet</servlet-name>
    <servlet-class>com.revature.servlets.FinanceManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FinanceManagerServlet</servlet-name>
    <url-pattern>/manager/*</url-pattern>
  </servlet-mapping>
</web-app>